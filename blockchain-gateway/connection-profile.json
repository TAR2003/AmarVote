{
  "name": "amarvote-network",
  "version": "1.0.0",
  "client": {
    "organization": "Org1",
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        }
      }
    }
  },
  "organizations": {
    "Org1": {
      "mspid": "Org1MSP",
      "peers": [
        "peer0.org1.amarvote.com",
        "peer1.org1.amarvote.com"
      ],
      "certificateAuthorities": [
        "ca.org1.amarvote.com"
      ]
    }
  },
  "orderers": {
    "orderer.amarvote.com": {
      "url": "grpcs://orderer.amarvote.com:7050",
      "grpcOptions": {
        "ssl-target-name-override": "tlsca.amarvote.com"
      },
      "tlsCACerts": {
        "path": "/app/crypto-local/ordererOrganizations/amarvote.com/orderers/orderer.amarvote.com/msp/tlscacerts/tlsca.amarvote.com-cert.pem"
      }
    }
  },
  "peers": {
    "peer0.org1.amarvote.com": {
      "url": "grpcs://peer0.org1.amarvote.com:7051",
      "grpcOptions": {
        "ssl-target-name-override": "tlsca.org1.amarvote.com",
        "hostnameOverride": "peer0.org1.amarvote.com"
      },
      "tlsCACerts": {
        "path": "/app/crypto-local/peerOrganizations/org1.amarvote.com/peers/peer0.org1.amarvote.com/tls/ca.crt"
      }
    },
    "peer1.org1.amarvote.com": {
      "url": "grpcs://peer1.org1.amarvote.com:8051",
      "grpcOptions": {
        "ssl-target-name-override": "peer1.org1.amarvote.com",
        "hostnameOverride": "peer1.org1.amarvote.com"
      },
      "tlsCACerts": {
        "path": "/app/crypto-local/peerOrganizations/org1.amarvote.com/peers/peer1.org1.amarvote.com/tls/ca.crt"
      }
    }
  },
  "certificateAuthorities": {
    "ca.org1.amarvote.com": {
      "url": "http://ca.org1.amarvote.com:7054",
      "caName": "ca-org1"
    }
  },
  "channels": {
    "amarvotechannel": {
      "orderers": [
        "orderer.amarvote.com"
      ],
      "peers": {
        "peer0.org1.amarvote.com": {
          "endorsingPeer": true,
          "chaincodeQuery": true,
          "ledgerQuery": true,
          "eventSource": true
        },
        "peer1.org1.amarvote.com": {
          "endorsingPeer": true,
          "chaincodeQuery": true,
          "ledgerQuery": true,
          "eventSource": true
        }
      }
    }
  }
}
